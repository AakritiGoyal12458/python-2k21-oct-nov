#wrong way of copying the list

foods=['pizza','cake','dark-choclate','pasta','chowmein']
food2=foods
print("this list is foods" ,foods)
print("this list is food2" ,food2)
foods.append("marshmello")
print("after adiing marshmello in list foods")
print("this list is foods" ,foods)
print("this list is food2" ,food2)
food2.append("maggiee")
print("after adiing maggiee in list food2")
print("this list is foods" ,foods)
