vegetables=['tomato','cabbage','cauliflower','ladyfinger','brinjal']

vegetables.append("corrainder")
vegetables.append("red chiili")

vegetables.insert(2,"green chillies")

vegetables.insert(2,"tomato")

print(vegetables)

#remove the elements

vegetables.remove("tomato")


del vegetables[0]
print(vegetables)

store_varibale= vegetables.pop(2)
print(vegetables)
print(store_varibale)


#acess element using negative indexing

print(vegetables[-1])
print(vegetables[-2])

#try to insert element in the end of list using negative indexing ...
vegetables.insert(-2,"tomato")
vegetables.insert(-1,"beetroot")

print(len(vegetables))#8
vegetables.insert(8,"bellpeper")


vegetables.insert(len(vegetables),"yellow bell pepper ")
print(vegetables)















































